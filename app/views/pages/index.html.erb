<div class="calendar-container">
 <%=  link_to "Back", back_path  %> 
  <%= month_calendar do |date, meetings| %>
    <%= date %>

    <% meetings.each do |meeting| %>
      <div class="meeting">
        <p><%= meeting.name %></p>
        <small><%= meeting.time unless meeting.multi_days? %></small>
      </div>
    <% end %>
  <% end %>
</div>